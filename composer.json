{
  "name": "maikschneider/ms-lebenswurzel",
  "description": "TYPO3 extension for LebensWurzel e.V.",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Maik Schneider",
      "email": "schneider.maik@me.com",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-about": "^10.4",
    "typo3/cms-adminpanel": "^10.4",
    "typo3/cms-backend": "^10.4",
    "typo3/cms-belog": "^10.4",
    "typo3/cms-beuser": "^10.4",
    "typo3/cms-core": "^10.4",
    "typo3/cms-dashboard": "^10.4",
    "typo3/cms-extbase": "^10.4",
    "typo3/cms-extensionmanager": "^10.4",
    "typo3/cms-filelist": "^10.4",
    "typo3/cms-fluid": "^10.4",
    "typo3/cms-fluid-styled-content": "^10.4",
    "typo3/cms-form": "^10.4",
    "typo3/cms-frontend": "^10.4",
    "typo3/cms-impexp": "^10.4",
    "typo3/cms-info": "^10.4",
    "typo3/cms-install": "^10.4",
    "typo3/cms-lowlevel": "^10.4",
    "typo3/cms-opendocs": "^10.4",
    "typo3/cms-recordlist": "^10.4",
    "typo3/cms-redirects": "^10.4",
    "typo3/cms-reports": "^10.4",
    "typo3/cms-rte-ckeditor": "^10.4",
    "typo3/cms-scheduler": "^10.4",
    "typo3/cms-seo": "^10.4",
    "typo3/cms-setup": "^10.4",
    "typo3/cms-tstemplate": "^10.4",
    "typo3/cms-viewpage": "^10.4",
    "gridelementsteam/gridelements": "^10.0",
    "georgringer/news": "^9.0",
    "blueways/deployer-recipes": "^2.0",
    "blueways/bw-placeholder-images": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "MaikSchneider\\MsLebenswurzel\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public",
      "extension-key": "ms_lebenswurzel"
    },
    "helhum/dotenv-connector": {
      "env-file": ".env",
      "adapter": "Helhum\\DotEnvConnector\\Adapter\\SymfonyDotEnv"
    }
  },
  "config": {
    "vendor-dir": "vendor",
    "allow-plugins": {
      "typo3/*": true,
      "helhum/dotenv-connector": true,
      "helhum/typo3-console-plugin": true
    }
  },
  "scripts": {
    "typo3-cms-scripts": [
      "ln -sfn ../../../ public/typo3conf/ext/ms_lebenswurzel",
      "vendor/bin/typo3cms install:fixfolderstructure"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  }
}
